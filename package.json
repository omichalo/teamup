{
  "name": "teamup",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "check": "npm run lint && npm run type-check && npm run build",
    "check:dev": "npm run lint && npm run type-check",
    "dev": "next dev",
    "dev:no-turbo": "next dev",
    "build": "next build",
    "deploy:firestore:prod": "firebase deploy --only firestore:rules,firestore:indexes --project sqyping-teamup",
    "deploy:firestore:dev": "firebase deploy --only firestore:rules,firestore:indexes --project sqyping-teamup-dev",
    "build:strict": "tsc --noEmit && next build",
    "type-check": "tsc --noEmit",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "functions:setup": "node scripts/setup-functions-config.js",
    "functions:build": "cd functions && npm run build",
    "functions:deploy": "node scripts/deploy-functions.js",
    "functions:emulator": "cd functions && npm run serve",
    "functions:test": "node scripts/test-functions-local.js",
    "functions:logs": "cd functions && npm run logs",
    "deploy:apphosting": "firebase deploy --only apphosting",
    "discord:register-command": "TS_NODE_TRANSPILE_ONLY=1 ts-node scripts/register-discord-command.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource-variable/figtree": "^5.2.10",
    "@hookform/resolvers": "^5.2.2",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@omichalo/ffttapi-node": "^0.0.9",
    "@omichalo/sqyping-mui-theme": "^0.2.8",
    "@types/react-beautiful-dnd": "^13.1.8",
    "discord.js": "^14.25.0",
    "firebase": "^12.2.1",
    "firebase-admin": "^12.0.0",
    "next": "15.5.3",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.10",
    "react": "19.1.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "swagger-ui-dist": "^5.30.2",
    "swagger-ui-react": "^5.30.2",
    "tweetnacl": "^1.0.3",
    "yup": "^1.7.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.3",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/swagger-ui-react": "^5.18.0",
    "dotenv": "^17.2.2",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}